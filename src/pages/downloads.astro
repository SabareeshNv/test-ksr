---
import BaseLayout from "@layouts/BaseLayout.astro";

const title =
    "Download KSR (Kerala Service Rules) PDF | Kerala Service Rules in Malayalam | KeralaServiceRules.com";
const description =
    "Download Kerala Service Rules (KSR) PDF. | Kerala Service Rules in Malayalam | KeralaServiceRules.com";
---

<BaseLayout {title} {description}>
    <div class="wrapper page-columns my-8">
        <main>
            <h1 class="text-3xl">
                Download Kerala Service Rules PDF from below.
            </h1>
            <table class="table-auto md:table-fixed mt-8 text-left">
                <thead class="uppercase text-xl">
                    <tr>
                        <th class="px-6 py-3">Sl No</th>
                        <th class="px-6 py-3">Book Edition</th>
                        <th class="px-6 py-3">Download</th>
                    </tr>
                </thead>
                <tbody class="text-xl">
                    <tr class="">
                        <td class="px-6 py-4">1</td>
                        <td class="px-6 py-4"
                            >Kerala Service Rules Volume 1 - 8th Edition</td
                        >
                        <td class="px-6 py-4">Dowload from Here</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-4">2</td>
                        <td class="px-6 py-4"
                            >Kerala Service Rules Volume 2 - 6th Edition</td
                        >
                        <td class="px-6 py-4">Dowload from Here</td>
                    </tr>
                </tbody>
            </table>
        </main>
        <aside></aside>
    </div>
</BaseLayout>
